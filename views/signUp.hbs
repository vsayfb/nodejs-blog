<div class="container">
  <div class="center">
    <form class="a-form" onsubmit="signUp(event)" id="signUp">
      <h1>Sign Up</h1>
      <div class="a-form-input">
        <div>
          <label for="name" class="">Name</label>
        </div>
        <input type="text" class="" name="name" />
      </div>
      <div class="a-form-input">
        <div>
          <label for="displayName" class="">Display name</label>
        </div>
        <input type="text" class="" name="displayName" />
      </div>

      <div class="a-form-input">
        <div>
          <label for="email" class="">Email address</label>
        </div>
        <input type="email" class="" name="email" />
      </div>

      <div class="a-form-input">
        <div>
          <label for="password" class="">Password</label>
        </div>
        <input type="password" class="" name="password" />
      </div>
      <button type="submit" class="yellow-button">Submit</button>
    </form>
  </div>
</div>

<script src="/js/signUp.js"></script>